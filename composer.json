{
  "name": "kdt-lib/queue",
  "repositories": {"packagist": {"type": "composer", "url": "http://packagist.qima-inc.com"}},
  "config": { "secure-http": false },
  "require": {
    "react/react": ">=0.2.1",
    "kdt-lib/cache": "~1.0.0",
    "kdt-lib/config": "~1.0.0",
    "kdt-lib/tracing": "~1.0.0",
    "zanphp/service-chain": "1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.9"
  },
  "autoload": {
    "psr-4": {
      "Kdt\\Iron\\Queue\\": "src"
    },
    "psr-0": {
      "nsqphp": "src/Adapter/nsqphp/src"
    }
  },
  "autoload-dev": {
    "files": ["tests/classes/env.php"],
    "psr-4": {
      "Kdt\\Iron\\Queue\\Tests\\": "tests/source",
      "Kdt\\Iron\\Queue\\Tests\\classes\\": "tests/classes"
    }
  }
}
